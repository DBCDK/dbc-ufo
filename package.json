  "main": "src/main.js",
  "scripts": {
    "dev": "npm run build:dev & . ./env.env && npm run serve",
    "lint": "eslint ./src ./integration/ --cache",
    "serve": "nodemon --exec node --no-stdin src/main.js",
    "build": "webpack --define process.env.NODE_ENV='\"production\"' --progress --colors -p",
    "build:dev": "webpack --progress --colors --watch -d",
    "precommit": "npm run lint && npm run test && npm run test:integration:local",
    "test": ". ./test.env && mocha --recursive ./src/**/*.test.js --compilers js:babel-register",
    "test:integration": ". ./test.env && npm run build && TEST_ENV=saucelabs wdio ./integration/config/wdio.conf.js",
    "test:integration:local": "rm -rf errorShots/*.png && . ./env.env && . ./test.env && TEST_ENV=local wdio ./integration/config/wdio.conf.js",
    "test:integration:jenkins": "npm run build && TEST_ENV=jenkins wdio ./integration/config/wdio.conf.js"
  },
